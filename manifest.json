{
  "name": "ThatSong",
  "short_name": "ThatSong",
  "options_page": "options.html",
  "version": "1.0.2",
  "description": "DJ's Best Fwend",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy":
    "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/thatSong_16.png",
      "32": "images/thatSong_32.png",
      "48": "images/thatSong_48.png",
      "128": "images/thatSong_128.png"
    }
  },
  "icons": {
    "16": "images/thatSong_16.png",
    "32": "images/thatSong_32.png",
    "48": "images/thatSong_48.png",
    "128": "images/thatSong_128.png"
  },
  "manifest_version": 2
}
